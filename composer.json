{
    "name": "artisanry/token-session",
    "description": "",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-token-session",
        "Laravel Token Session"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "illuminate/session": "^5.8|^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\TokenSession\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\TokenSession\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\TokenSession\\TokenSessionServiceProvider"
            ],
            "aliases": {
                "TokenSession": "Artisanry\\TokenSession\\Facades\\TokenSession"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
